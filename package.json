    "@types/react-dom": "^18.0.8",
    "@types/react-instantsearch-dom": "^6.12.3",
    "@types/remove-markdown": "^0.3.1",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-next": "^13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "github-slugger": "^2.0.0",
