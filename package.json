{
  "name": "blog.oriverk.dev",
  "private": true,
  "description": "tech blog for portfolio",
  "keywords": [
    "React",
    "Next.js",
    "TypeScript",
    "TailwindCSS"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oriverk/blog.oriverk.dev.git"
  },
  "bugs": {
    "url": "https://github.com/oriverk/blog.oriverk.dev/issues"
  },
  "homepage": "https://github.com/oriverk/blog.oriverk.dev#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:prettier": "prettier --check {src,app,script}/**/*.{js,jsx,ts,tsx}",
    "lint:eslint": "eslint {src,app,script}/**/*.{js,jsx,ts,tsx}",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:md": "run-s fix:markdownlint fix:textlint",
    "fix:prettier": "prettier --write {src,app,script}/**/*.{js,jsx,ts,tsx}",
    "fix:eslint": "eslint --fix {src,app,script}/**/*.{js,jsx,ts,tsx}",
    "fix:markdownlint": "markdownlint -fix docs/**",
    "fix:textlint": "textlint --fix docs/**",
    "postsmap": "ts-node --project tsconfig.module.json script/genPostsMap.ts",
    "algolia": "ts-node --project tsconfig.module.json script/push2Algolia.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.17",
    "@next/bundle-analyzer": "^13.2.4",
