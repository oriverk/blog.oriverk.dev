{
  "name": "blog.oriverk.dev",
  "private": true,
  "description": "tech blog for portfolio",
  "keywords": [
    "React",
    "Next.js",
    "TypeScript",
    "TailwindCSS"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oriverk/blog.oriverk.dev.git"
  },
  "bugs": {
    "url": "https://github.com/oriverk/blog.oriverk.dev/issues"
  },
  "homepage": "https://github.com/oriverk/blog.oriverk.dev#readme",
  "scripts": {
    "dev": "next dev",
    "generate:getContents": "node script/getGitHubContents.mjs",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:prettier": "prettier --check {src,app,script}/**/*.{js,jsx,ts,tsx}",
    "lint:eslint": "eslint {src,app,script}/**/*.{js,jsx,ts,tsx}",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:md": "run-s fix:markdownlint fix:textlint",
    "fix:prettier": "prettier --write {src,app,script}/**/*.{js,jsx,ts,tsx}",
    "fix:eslint": "eslint --fix {src,app,script}/**/*.{js,jsx,ts,tsx}",
    "fix:markdownlint": "markdownlint -fix docs/**/*",
    "fix:textlint": "textlint --fix docs/**/*",
    "debug": "textlint --fix docs/2023/202304-downloader-site.md --debug",
    "postsmap": "ts-node --project tsconfig.module.json script/genPostsMap.ts",
    "algolia": "ts-node --project tsconfig.module.json script/push2Algolia.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@next/bundle-analyzer": "^13.4.19",
    "@octokit/graphql": "^7.0.1",
    "algoliasearch": "^4.19.1",
    "date-fns": "^2.30.0",
    "fetch-site-metadata": "^0.1.3",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "next": "^13.4.19",
    "next-mdx-remote": "^4.4.1",
    "prism-react-renderer": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-instantsearch-hooks-web": "^6.45.0",
    "react-use": "^17.4.0",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-comment": "^1.0.0",
    "remark-gfm": "^3.0.1",
    "rss": "^1.2.2",
    "tailwind-merge": "^1.14.0",
    "unified": "^11.0.2",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@proofdict/textlint-rule-proofdict": "^3.1.2",
    "@tailwindcss/typography": "^0.5.9",
    "@textlint/textlint-plugin-markdown": "^13.3.3",
    "@types/github-slugger": "^1.3.0",
    "@types/node": "^20.5.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-instantsearch-dom": "^6.12.3",
    "@types/remove-markdown": "^0.3.1",
    "@types/rss": "^0.0.30",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "markdownlint-cli": "^0.35.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "remove-markdown": "^0.5.0",
    "sass": "^1.66.1",
    "sharp": "^0.32.5",
    "tailwindcss": "^3.3.3",
    "textlint": "^13.3.3",
    "textlint-rule-aws-service-name": "^1.4.0",
    "textlint-rule-no-kangxi-radicals": "^0.2.2",
    "textlint-rule-preset-ja-technical-writing": "^8.0.0",
    "textlint-rule-preset-smarthr": "^1.26.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
