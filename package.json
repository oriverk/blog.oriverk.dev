  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:prettier": "prettier --check {src,pages,script}/**/*.{js,jsx,ts,tsx}",
    "lint:eslint": "eslint {src,pages,script}/**/*.{js,jsx,ts,tsx}",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:md": "run-s fix:markdownlint fix:textlint",
    "fix:prettier": "prettier --write {src,pages,script}/**/*.{js,jsx,ts,tsx}",
    "fix:eslint": "eslint --fix {src,pages,script}/**/*.{js,jsx,ts,tsx}",
    "fix:markdownlint": "markdownlint -fix docs/**",
    "fix:textlint": "textlint --fix docs/**",
    "postsmap": "ts-node --project tsconfig.module.json script/genPostsMap.ts",
    "algolia": "ts-node --project tsconfig.module.json script/push2Algolia.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@next/bundle-analyzer": "^13.0.2",
