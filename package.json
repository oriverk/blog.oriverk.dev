{
  "name": "blog.oriverk.dev",
  "private": true,
  "description": "tech blog for portfolio",
  "keywords": [
    "React",
    "Next.js",
    "TypeScript",
    "goober"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oriverk/blog.oriverk.dev.git"
  },
  "bugs": {
    "url": "https://github.com/oriverk/blog.oriverk.dev/issues"
  },
  "homepage": "https://github.com/oriverk/blog.oriverk.dev#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:prettier": "prettier --check {src,pages}/**/*",
    "lint:eslint": "eslint {src,pages}/**/*",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:md": "run-s fix:markdownlint fix:textlint",
    "fix:prettier": "prettier --write {src,pages}/**/*",
    "fix:eslint": "eslint --fix {src,pages}/**/*",
    "fix:markdownlint": "markdownlint -fix docs/**",
    "fix:textlint": "textlint --fix docs/**",
    "algolia": "node script/genPostsMap.js && node script/push2Algolia.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "algoliasearch": "^4.13.0",
    "date-fns": "^2.28.0",
    "github-slugger": "^1.4.0",
    "goober": "^2.1.9",
    "gray-matter": "^4.0.3",
    "next": "^12.1.6",
    "next-head-seo": "^0.1.3",
    "next-mdx-remote": "^4.0.3",
    "prism-react-renderer": "^1.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-instantsearch-dom": "^6.24.2",
    "react-use": "^17.3.2",
    "rehype-slug": "^5.0.1",
    "remark-comment": "^1.0.0",
    "remove-markdown": "^0.5.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/github-slugger": "^1.3.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-instantsearch-dom": "^6.12.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "markdownlint-cli": "^0.31.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "sharp": "^0.30.5",
    "textlint": "^12.1.1",
    "textlint-rule-preset-ja-spacing": "^2.2.0",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "typescript": "^4.6.4"
  }
