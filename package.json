    "prettier-plugin-tailwindcss": "^0.1.13",
    "prism-react-renderer": "^1.3.5",
    "rehype-slug": "^5.1.0",
    "remark-comment": "^1.0.0",
    "remark-gfm": "^3.0.1",
    "remove-markdown": "^0.5.0",
    "sass": "^1.56.1",
    "sharp": "^0.31.2",
    "tailwindcss": "^3.2.4",
    "textlint": "^12.2.2",
    "textlint-rule-preset-ja-spacing": "^2.2.0",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
