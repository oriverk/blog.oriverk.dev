{
  "name": "blog.oriverk.dev",
  "private": true,
  "description": "tech blog for portfolio",
  "keywords": ["React", "Next.js", "TypeScript", "goober"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oriverk/oriverk.dev.git"
  },
  "bugs": {
    "url": "https://github.com/oriverk/oriverk.dev/issues"
  },
  "homepage": "https://github.com/oriverk.dev/site#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:prettier": "prettier --check {src,pages}/**/*",
    "lint:eslint": "eslint {src,pages}/**/*",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:md": "run-s fix:markdownlint fix:textlint",
    "fix:prettier": "prettier --write {src,pages}/**/*",
    "fix:eslint": "eslint --fix {src,pages}/**/*",
    "fix:markdownlint": "markdownlint -f docs/*",
    "fix:textlint": "textlint -f docs/*",
    "algolia": "node script/genPostsMap.js && node script/push2Algolia.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.0.7",
    "algoliasearch": "^4.3.0",
    "date-fns": "^2.13.0",
    "github-slugger": "^1.4.0",
    "goober": "^2.1.1",
    "gray-matter": "^4.0.2",
    "next": "^12.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^3.0.8",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-instantsearch-dom": "^6.10.0",
    "react-use": "^17.3.1",
    "rehype-slug": "^5.0.0",
    "remark-unwrap-images": "^3.0.1",
    "remove-markdown": "^0.3.0"
  },
  "devDependencies": {
    "@types/github-slugger": "^1.3.0",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-instantsearch-dom": "^6.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "markdownlint-cli": "^0.30.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "sharp": "^0.29.3",
    "textlint": "^12.1.0",
    "textlint-rule-preset-ja-spacing": "^2.2.0",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "typescript": "^4.4.4"
  }
